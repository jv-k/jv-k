{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown", "./node_modules/better-docs"],
  "templates": {
    "better-docs": {
      "name": "GitHub Readme Skillset Icons Generator"
    }
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/better-docs"
  }
}